# export CC_FOR_BUILD=clang
# Compile Kernel
export ARCH=arm64
export SUBARCH=arm64
export KBUILD_BUILD_USER=Nobita
export KBUILD_BUILD_HOST=aarch64
export CROSS_COMPILE=aarch64-linux-gnu-
export CROSS_COMPILE_ARM32=arm-linux-gnu-
# export CC_FOR_BUILD=clang-11
export CLANG_PATH=/usr/bin
export export ANDROID_MAJOR_VERSION=p
alias whyred-old="make O=out whyred-old_defconfig"
alias whyred-old="make O=out whyred-new_defconfig"
alias nanowhyred="nano */*/*/whyred-perf*"
alias menuconfig="make O=out menuconfig"
alias compile="make O=out -j$(nproc --all) | tee out/build.log"
alias mcompile="make O=out -j4 | tee out/build.log"
export KBUILD_COMPILER_STRING=$(/usr/bin/clang --version | head -n 1 | perl -pe 's/\(http.*?\)//gs' | sed -e 's/  */ /g' -e 's/[[:space:]]*$//')
alias ccompile="make -j$(nproc --all) O=out ARCH=arm64 CC="/usr/bin/clang" CLANG_TRIPLE="aarch64-linux-gnu-" | tee out/build.log"
alias mccompile="make -j4 O=out ARCH=arm64 CC="/usr/bin/clang" CLANG_TRIPLE="aarch64-linux-gnu-" | tee out/build.log"
alias kernelversion="make kernelversion"
alias pick="git cherry-pick"
alias picke="git cherry-pick -e"
alias cm="git commit"
alias cms="git commit -s"
alias cmsa="git commit -s --author="
alias pf="git push --force"
alias p="git push -u"
alias zimage="sudo cp -rv out/arch/arm64/boot/Image.gz-dtb doraemon/anykernel3/"
alias rzimage="sudo rm -rvf doraemon/anykernel3/Image.gz-dtb"
alias done_old="zip -rv9 Doraemon-Kernel-OldCam-$(date +"%m""%d"-"%H""%M").zip doraemon/anykernel3/* && mv Doraemon-Kernel-* doraemon/Download/ "
alias done_new="zip -rv9 Doraemon-Kernel-NewCam-$(date +"%m""%d"-"%H""%M").zip doraemon/anykernel3/* && mv Doraemon-Kernel-* doraemon/Download/ "
